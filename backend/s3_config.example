# AWS S3 Configuration for Course Content Storage
# Copy this file to .env and fill in your AWS credentials

# AWS Credentials
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_REGION=us-east-1

# S3 Bucket Configuration
S3_BUCKET_NAME=operator-skills-hub-content

# Optional: S3 Configuration
# S3_ENDPOINT_URL=https://s3.amazonaws.com  # For S3-compatible services
# S3_FORCE_PATH_STYLE=false  # Set to true for S3-compatible services
